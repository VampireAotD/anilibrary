{
    "printWidth": 90,
    "trailingComma": "es5",
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "endOfLine": "lf",
    "plugins": [
        "@trivago/prettier-plugin-sort-imports"
    ],
    "importOrder": [
        "^(vue|@vue|vitest)",
        "<THIRD_PARTY_MODULES>",
        "^@/(pages|widgets|features|entities|shared|types|mocks)",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
