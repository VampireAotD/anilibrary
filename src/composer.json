{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "vampireaotd/anilibrary",
    "type": "project",
    "description": "Application for scraping anime data.",
    "keywords": [
        "anilibrary",
        "anime",
        "library",
        "laravel"
    ],
    "license": "BSL-1.0",
    "require": {
        "php": "^8.3",
        "aws/aws-sdk-php": "^3.342.26",
        "cloudinary-labs/cloudinary-laravel": "^3.0.0",
        "diglactic/laravel-breadcrumbs": "^10.0",
        "elasticsearch/elasticsearch": "^8.17.1",
        "firebase/php-jwt": "^6.11.1",
        "guzzlehttp/guzzle": "^7.9.3",
        "inertiajs/inertia-laravel": "^2.0.2",
        "laravel/framework": "v12.8.1",
        "laravel/horizon": "^5.31.1",
        "laravel/octane": "^2.9.0",
        "laravel/reverb": "^1.5.0",
        "laravel/sanctum": "^4.0.8",
        "laravel/serializable-closure": "^2.0.3",
        "laravel/tinker": "^2.10.1",
        "nutgram/laravel": "^1.5.0",
        "pusher/pusher-php-server": "^7.2.7",
        "sentry/sentry-laravel": "^4.13.0",
        "spatie/laravel-permission": "^6.17.0",
        "tightenco/ziggy": "^2.5.2",
        "tuupola/base62": "^2.1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15.3",
        "barryvdh/laravel-ide-helper": "^3.5.5",
        "brianium/paratest": "^v7.9.1",
        "driftingly/rector-laravel": "^2.0.4",
        "fakerphp/faker": "^1.24.1",
        "infection/infection": "^0.29.14",
        "larastan/larastan": "^3.3.1",
        "laravel/breeze": "^2.3.6",
        "laravel/pint": "^1.22.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.0",
        "phpunit/phpunit": "^12.1.2",
        "rector/rector": "^2.0.11"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models -M",
            "@php artisan ide-helper:eloquent"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-install-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi",
            "@ide-helper"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi",
            "@ide-helper"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true,
            "infection/extension-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
