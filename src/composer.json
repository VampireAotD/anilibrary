{
    "name": "vampireaotd/anilibrary",
    "type": "project",
    "description": "Application for scraping anime data.",
    "keywords": [
        "anilibrary",
        "anime",
        "library",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "aws/aws-sdk-php": "^3.281.3",
        "cloudinary-labs/cloudinary-laravel": "^2.0.3",
        "elasticsearch/elasticsearch": "^8.10",
        "firebase/php-jwt": "^6.9",
        "guzzlehttp/guzzle": "^7.8",
        "inertiajs/inertia-laravel": "^0.6.10",
        "laravel/framework": "^10.29",
        "laravel/horizon": "^5.21",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",
        "predis/predis": "^2.2",
        "pusher/pusher-php-server": "^7.2",
        "spatie/laravel-permission": "^6.0",
        "tightenco/ziggy": "^1.8",
        "tuupola/base62": "^2.1",
        "westacks/telebot-laravel": "^3.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9",
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^1.25",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6.2",
        "nunomaduro/collision": "^7.10",
        "nunomaduro/larastan": "^2.6",
        "php-http/mock-client": "^1.6",
        "phpunit/phpunit": "^10.4",
        "psalm/plugin-laravel": "^2.8",
        "spatie/laravel-ignition": "^2.3",
        "vimeo/psalm": "^5.15"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
