{
    "name": "vampireaotd/anilibrary",
    "type": "project",
    "description": "Application for scraping anime data.",
    "keywords": [
        "anilibrary",
        "anime",
        "library",
        "laravel"
    ],
    "license": "BSL-1.0",
    "require": {
        "php": "^8.3",
        "aws/aws-sdk-php": "^3.314",
        "cloudinary-labs/cloudinary-laravel": "^2.1",
        "elasticsearch/elasticsearch": "^8.14",
        "firebase/php-jwt": "^6.10",
        "guzzlehttp/guzzle": "^7.8",
        "inertiajs/inertia-laravel": "^1.3",
        "laravel/framework": "^11.11",
        "laravel/horizon": "^5.24",
        "laravel/octane": "^2.4",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "nutgram/laravel": "^1.4",
        "predis/predis": "^2.2",
        "pusher/pusher-php-server": "^7.2",
        "spatie/laravel-permission": "^6.7",
        "tightenco/ziggy": "^2.2",
        "tuupola/base62": "^2.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13",
        "barryvdh/laravel-ide-helper": "^3.0",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^2.9",
        "laravel/breeze": "^2.1",
        "laravel/pint": "^1.16",
        "laravel/sail": "^1.29",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "php-http/mock-client": "^1.6",
        "phpunit/phpunit": "^10.5",
        "psalm/plugin-laravel": "^2.11",
        "spatie/laravel-ignition": "^2.7",
        "vimeo/psalm": "^5.25"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models -M",
            "@php artisan ide-helper:eloquent"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-install-cmd": [
            "@ide-helper"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi",
            "@ide-helper"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
